@model Library.Models.State

<fieldset>
    <legend>State</legend>

    @Html.Label("Your currently data storage: " + Model.DataStorage)

    @if (Model.DataStorage.Equals("SESSION_CACHE"))
    {
        if (Model.MinutesLeft > 60)
        {
            @Html.Label("Expires in more than a hour")
        }
        else
        {
            @Html.Label("Expires expire in " + Model.MinutesLeft.ToString() + "minutes")
        }
    }

</fieldset>

