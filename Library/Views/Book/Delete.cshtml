@model Library.Models.Book

@{
    ViewBag.Title = "Entfernen";
}

<h2>@ViewBag.Title</h2>

<h3>Sind Sie sicher, dass Sie dieses Buch entfernen möchten?</h3>

@Html.Partial("Partial/_details", Model)

<h4>Durch diese Acktion wird ebenfalls der Ausleihverlauf des Buches entfernt.</h4>
 
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Helpers.deleteActions(new { id = Model.id })
}